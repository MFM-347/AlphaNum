{
  "name": "@tfs-8/alphanum",
  "version": "1.0.0",
  "description": "A lightweight JavaScript utility for encoding and decoding alphanumeric text into numeric representations.",
  "type": "module",
  "main": "./dist/alpha-num.umd.js",
  "module": "./dist/alpha-num.es.js",
  "types": "./dist/alphaNum.d.ts",
  "exports": {
    ".": {
      "import": "./dist/alpha-num.es.js",
      "require": "./dist/alpha-num.umd.js"
    }
  },
  "files": [
    "dist/alpha-num.umd.js",
    "dist/alpha-num.es.js",
    "README.md"
  ],
  "scripts": {
    "prebuild": "npm run format && npm run lint",
    "build": "tsc && vite build",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --fix",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.4.0"
  },
  "keywords": [
    "alpha",
    "numeric",
    "alphanumeric",
    "encode",
    "decode",
    "converter",
    "string",
    "number",
    "alphabet",
    "utility",
    "javascript",
    "text-utility",
    "mfm3478"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MFM-347/AlphaNum.git"
  },
  "bugs": {
    "url": "https://github.com/MFM-347/AlphaNum/issues"
  },
  "homepage": "https://github.com/MFM-347/AlphaNum#readme",
  "author": {
    "name": "Farhan Madni",
    "email": "madnifm347@outlook.com",
    "url": "https://github.com/MFM-347"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.17.0"
  },
  "packageManager": "npm@10.8.2"
}
